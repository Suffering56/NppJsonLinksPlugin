{
  "highlightingEnabled": true,
  "soundEnabled": true,
  "jumpToLineDelay": 0,
  "mappingDefaultFilePath": "F:/gd_data",
  "mapping": [
    {
      "description": "rooms.json: roomId",
      "dst": {
        "fileName": "rooms.json",
        "word": "id<<<ROOT"
      },
      "src": [
        {
          "wordRegexpEnabled": true,
          "fileName": "*.json",
          "word": "roomId<<<extension<<<^[\\w_]*restriction[\\w_]?$"
        }
      ]
    },
    {
      "description": "equipments.json: equipmentId",
      "dst": {
        "fileName": "equipments.json",
        "word": "id<<<ROOT"
      },
      "src": [
        {
          "fileName": "*.json",
          "word": "equipmentId"
        }
      ]
    },
    {
      "description": "offers.json: offerId",
      "dst": {
        "fileName": "offers.json",
        "word": "id<<<offers"
      },
      "src": [
        {
          "fileName": "*.json",
          "word": "offerId"
        }
      ]
    },
    {
      "description": "parametrizedMobs.json: defaultParamsKey",
