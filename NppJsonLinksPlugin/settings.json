      "dst": {
        "fileName": "parametrizedMobs.json",
        "word": "id<<<templates"
      },
      "src": [
        {
          "order": 1,   // parametrizedMobs [1] > mobSettings[2]
          "fileName": "*.json",
          "word": "specificMobId?"
        },
        {
          "order": 2,   // mobId: mobSettings[1] > parametrizedMobs[2] > summonSettings[3]
          "fileName": "*.json",
          "word": "mobId"
        }
      ]
    },
    {
      "description": "summonSettings.json: mobId",
      "dst": {
        "fileName": "summonSettings.json",
        "word": "id<<<ROOT"
      },
      "src": [
        {
          "order": 3,   // mobId: mobSettings[1] > parametrizedMobs[2] > summonSettings[3]
          "fileName": "*.json",
          "word": "mobId"
        }
      ]
    },
    {
      "description": "animals.json: animalId",
