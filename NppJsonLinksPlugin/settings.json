        {
          "fileName": "*.json",
          "word": "viewId"
        },
        {
          "fileName": "*.json",
          "word": "equipmentViewId"
        }
      ]
    },
    {
      "description": "mobSettings.json: mobId",
      "dst": {
        "fileName": "mobSettings.json",
        "word": "id<<<ROOT"
      },
      "src": [
        {
          "order": 1,   // mobId: mobSettings[1] > parametrizedMobs[2] > summonSettings[3]
          "fileName": "*.json",
          "word": "mobId"
        },
        {
          "order": 2,   // parametrizedMobs [1] > mobSettings[2]
          "fileName": "*.json",
          "word": "specificMobId?"
        }
      ]
    },
    {
